{
  "name": "docgen",
  "displayName": "VS Code JSDoc Generator",
  "description": "Generate comprehensive JSDoc documentation for your JavaScript and TypeScript code using AI",
  "version": "1.0.1",
  "publisher": "YOUR_PUBLISHER_ID",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other",
    "Documentation",
    "Programming Languages"
  ],
  "keywords": [
    "jsdoc",
    "documentation",
    "ai",
    "openai",
    "typescript",
    "javascript",
    "code-generation"
  ],
  "activationEvents": [
    "onCommand:extension.generateDocs"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.generateDocs",
        "title": "Generate JSDoc",
        "category": "JSDoc Generator"
      }
    ],
    "configuration": {
      "title": "JSDoc Generator",
      "properties": {
        "docgen.openaiApiKey": {
          "type": "string",
          "default": "",
          "description": "Your OpenAI API key for JSDoc Generator"
        },
        "docgen.openai.model": {
          "type": "string",
          "default": "gpt-3.5-turbo",
          "enum": [
            "gpt-3.5-turbo",
            "gpt-4"
          ],
          "description": "OpenAI model to use for JSDoc generation"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts",
    "test": "jest --coverage",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "author": "Your Name <your.email@example.com>",
  "dependencies": {
    "openai": "^5.10.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^24.1.0",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.8.3",
    "vsce": "^2.15.0"
  }
}
